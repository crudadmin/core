{
    "name": "crudadmin/framework",
    "type": "library",
    "description": "Package creating support for Admin Model migrations feature",
    "keywords": ["crudadmin", "admin-model", "model", "admin", "eloquent", "laravel"],
    "homepage": "https://crudadmin.com",
    "license": "MIT",
    "authors": [
        { "name": "Marek GogoÄ¾", "email": "info@marekgogol.sk" }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "^5.4|^6.0|^7.0",
        "doctrine/dbal": "^2.6.3",
        "intervention/image": "^2.3.0",
        "spatie/image-optimizer": "^1.1",
        "laravel/helpers": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7|~7.0|^8.0",
        "orchestra/testbench": "^3.8|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Admin\\Core\\": "src",
            "Admin\\Core\\Tests\\": "tests"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Admin\\Core\\Tests\\App\\": "tests/Stubs/app"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Admin\\Core\\Providers\\AppServiceProvider"
            ]
        }
    }
}
